{
    "name": "emedia/oxygen",
    "description": "Foundation Laravel platform with a Dashboard and User handling.",
    "type": "library",
	"license": "proprietary",
	"authors": [
		{
			"name": "Shane Perera",
			"email": "shane@elegantmedia.com.au"
		}
	],
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^2.11",
        "elegantmedia/oxygen-foundation": "^2.0",
        "elegantmedia/php-toolkit": "^1.0",
        "emedia/api": "^5.0",
        "emedia/app-settings": "^5.0",
        "emedia/devices-laravel": "^4.0",
        "emedia/formation": "^1.0",
        "emedia/laravel-media-manager": "^4.0",
        "emedia/multi-tenant": "^1.0",
        "google/recaptcha": "^1.2",
        "laravel/fortify": "^1.0",
        "laravel/framework": "^9.19",
		"laravel/sanctum": "^3.0",
        "silber/bouncer": "1.0.0-rc.13",
        "spatie/laravel-sluggable": "^3.3"
    },
    "require-dev": {
        "emedia/laravel-test-kit": "^2.0",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.0",
        "orchestra/testbench-browser-kit": "^7.0",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
	"autoload": {
		"psr-4": {
			"EMedia\\Oxygen\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\": "vendor/orchestra/testbench-core/laravel/app",
			"Tests\\": "tests/",
			"Setup\\": "setup/"
		},
		"files": [
			"vendor/mockery/mockery/library/helpers.php",
			"vendor/elegantmedia/oxygen-foundation/tests/Traits/MocksScoutEngines.php"
		]
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
    "repositories": [
		{
			"type": "vcs",
			"url": "git@bitbucket.org:elegantmedia/devices-laravel.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:elegantmedia/laravel-app-settings.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:elegantmedia/devices-laravel.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:elegantmedia/laravel-api-helpers.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:elegantmedia/laravel-media-manager.git"
		},
		{
			"type": "vcs",
			"url": "git@bitbucket.org:elegantmedia/laravel-test-kit.git"
		},
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/lotus.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/formation.git"
        },
		{
			"type": "vcs",
			"url": "git@bitbucket.org:elegantmedia/multitenant-laravel.git"
		}
	],
	"config": {
		"sort-packages": true
	},
	"scripts": {
		"test": "phpunit",
		"check-style": "phpcs src tests",
		"fix-style": "phpcbf src tests"
	},
	"extra": {
		"laravel": {
			"providers": [
				"EMedia\\Oxygen\\OxygenServiceProvider"
			]
		}
	}
}
