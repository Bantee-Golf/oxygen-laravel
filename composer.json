{
    "name": "emedia/oxygen",
    "description": "Foundation Laravel platform with a Dashboard and User handling.",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/devices-laravel.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/multitenant-laravel.git"
        },
        {
          "type": "vcs",
          "url": "git@bitbucket.org:elegantmedia/file-control-laravel.git"
        },
        {
          "type":"vcs",
          "url":"git@bitbucket.org:elegantmedia/mediamanager-laravel.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/laravel-api-helpers.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/lotus.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/formation.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/laravel-app-settings.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/laravel-generators.git"
        },
        {
            "type":"vcs",
            "url":"git@bitbucket.org:elegantmedia/quickdata-laravel.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/laravel-helpers.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:elegantmedia/php-helpers.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-debugbar": "^3.2",
        "cviebrock/eloquent-sluggable": "^7.0",
        "doctrine/dbal": "^2.9",
        "google/recaptcha": "^1.2",
        "laravel/framework": "^7.0",
        "laravel/ui": "^2.0",
        "laravelcollective/html": "^6.0",
        "silber/bouncer": "v1.0.0-rc.7",
        "webpatser/laravel-uuid": "^3.0",
        "emedia/api": "^3.0",
        "emedia/quick-data-laravel": "^3.0",
        "emedia/generators": "^3.0",
        "emedia/lotus": "^1.2",
        "emedia/multi-tenant": "^0.3.0",
        "emedia/file-control": "^3.0",
        "emedia/devices-laravel": "^1.1",
        "emedia/app-settings": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "EMedia\\Oxygen\\OxygenServiceProvider"
            ]
        }
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Shane Perera",
            "email": "shane@elegantmedia.com.au"
        }
    ],
    "autoload": {
        "psr-4": {
            "EMedia\\Oxygen\\": "src/",
            "EMedia\\Oxygen\\Database\\": "database/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "LaravelDefaultFiles/app/",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
